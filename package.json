{
  "name": "dilia",
  "version": "0.1.0",
  "description": "Editor for assets used by Sarosa and Lycan",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server",
    "prepublish": "npm run typings",
    "typings": "typings install && cd server/ && typings install && cd ..",
    "test": "npm run karma && npm run lint",
    "lint": "tslint --project tsconfig.json && tslint --project server/tsconfig.json",
    "karma": "karma start --single-run",
    "build": "npm run build-client && npm run build-server",
    "build-client": "webpack --prod --bail",
    "server": "cd build && node ./server/server.js",
    "debug-server": "node --debug-brk ./build/server/server.js",
    "build-server": "cd server/ && tsc && cd .."
  },
  "repository": {
    "type": "git",
