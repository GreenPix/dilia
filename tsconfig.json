        "./editor/components/map/chipset.ts",
        "./editor/components/map/createnewmap.ts",
        "./editor/components/map/editor-state.ts",
        "./editor/components/map/editor.ts",
        "./editor/components/map/layers-panel.ts",
        "./editor/components/map/map-settings.ts",
        "./editor/components/map/panel-state.ts",
        "./editor/components/modal/dialog.ts",
        "./editor/components/profile/profile.ts",
        "./editor/components/rules/ace.ts",
        "./editor/components/rules/autocompleter.ts",
        "./editor/components/rules/behaviour.ts",
        "./editor/components/rules/commit.ts",
        "./editor/components/rules/editor.ts",
        "./editor/components/rules/exec.ts",
        "./editor/components/rules/globals.ts",
        "./editor/components/rules/interfaces.ts",
        "./editor/components/toolbar/toolbar.ts",
        "./editor/components/upload/upload.ts",
        "./editor/components/webgl/surface.ts",
        "./editor/components/webgl/zoom.ts",
        "./editor/gl/buffer.ts",
        "./editor/gl/draw.ts",
        "./editor/gl/gl.ts",
        "./editor/gl/program.ts",
        "./editor/gl/tex.ts",
        "./editor/gl/uniform.ts",
        "./editor/index.ts",
        "./editor/libs-free.ts",
        "./editor/models/map.ts",
        "./editor/models/scripting.ts",
        "./editor/models/user.ts",
        "./editor/rendering/camera.ts",
        "./editor/rendering/context.ts",
        "./editor/rendering/fbo.ts",
