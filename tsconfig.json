        "./editor/services/directives.ts",
        "./editor/services/index.ts",
        "./editor/shared.ts",
        "./editor/util/injector.ts",
        "./editor/util/mouse.ts",
        "./editor/util/requestAnimationFrame.ts",
        "./shared/aariba.ts",
        "./shared/map.ts",
        "./shared/socket.ts",
        "./typings/browser.d.ts",
        "./custom_typings/ace-custom.d.ts",
        "./custom_typings/webpack.d.ts"
